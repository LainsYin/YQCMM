QComboBox#Combobox{
    border:1px solid rgb(170, 170, 170);
    border-radius:5px;
    font-size:14px;
    color:rgb(88, 88, 88);    
}
QComboBox#Combobox::down-arrow{
    image:url(:/arrow/images/combobox_down.png);
}
QComboBox#Combobox::down-arrow:on{
    image:url(:/arrow/images/combobox_up.png);
}
QComboBox#Combobox QAbstractItemView{
     border:1px solid rgb(255, 146, 62);
     selection-color:rgb(255, 255, 255);
     selection-background-color:rgb(255, 146, 62);
     color:rgb(00, 00, 00);
}

QComboBox#Combobox QAbstractItemView::item{
        height:30px;
        font-size:10px;
}

QComboBox#Combobox::drop-down{
    width:20px;
    border:none;
}
QComboBox#Combobox:on{
    border:1px solid rgb(255, 146, 62);
    border-top-left-radius:5px;
    border-top-right-radius:5px;
    border-bottom-left-radius:0px;
    border-bottom-right-radius:0px;
}

QComboBox#Combobox:hover{
    border:1px solid rgb(42, 42, 42);
    color:rgb(42, 42, 42);
}
QComboBox#Combobox:disabled{
    border:1px solid rgb(201, 201, 201);
    color:rgb(225, 225, 225);
}

QLabel#TooptipLabel{
    background: transparent;
}

QLabel#CenterLabel{
    background-color:rgb(248, 248, 248);
    border-top: 1px solid rgb(201, 201, 201);
    border-bottom: 1px solid rgb(201, 201, 201);
}

QLabel#Label{
    background: transparent;
    font-size: 14px;
}

QTableView#TableView{
        background: transparent;
        border:0px;
}

TableView#PagingTable{
    background-color:rgb(247, 246, 246);
    alternate-background-color:rgb(234, 234, 234);
    font-size:14px;
}

TableView#PagingTable QHeaderView::section{
    background-color:rgb(234, 234, 234);
    border:0px;
    height:42px;
    font-size:14px;
}


TableView#LibPagingTable{
    background-color:rgb(247, 246, 246);
    border: 1px solid rgb(110, 161, 240);
    font-size:26px;
}

TableView#LibPagingTable::item{
    spacing:10px;
    border: 1px solid rgb(110, 161, 240);
}
TableView#LibPagingTable::item:selected
{
        color: #ffffff;
        background-color: #122A3A;
}


QScrollBar:vertical
{
    width: 9px;
    background: transparent;
    padding-top: 0px;
    padding-bottom: 0px;

}
QScrollBar::handle:vertical
{
    width: 9px;
    min-height: 20px;
    background: rgb(190, 190, 190);
}
QScrollBar::handle:vertical:hover
{
    background: rgb(170, 170, 170);
}
QScrollBar::sub-line:vertical
{
    height: 9px;
    width: 9px;
    subcontrol-position: top;
}
QScrollBar::add-line:vertical
{
    height: 9px;
    width: 9px;
    subcontrol-position: bottom;
}
QScrollBar::sub-line:vertical:hover
{
    background: rgb(190, 190, 190);
}
QScrollBar::add-line:vertical:hover
{
    background: rgb(190, 190, 190);
}
QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical
{
    background: rgb(220, 220, 220);
}

