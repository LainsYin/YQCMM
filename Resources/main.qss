QWidget#MainWidget{
    background-color: rgb(235, 235, 235);
}

QListView#ListView{
        font-size:14px;
        color:rgb(58, 58, 58);
        border: 1px solid rgb(201, 201, 201);
        background: rgb(248, 248, 248);
}

QStackedWidget#StackedWidget{
    font-size:14px;
    color:rgb(58, 58, 58);
    border: 1px solid rgb(201, 201, 201);
    background: rgb(248, 248, 248);
    border-radius:5px;
}


QPushButton#Button{
    background-color: rgb(255, 255, 255);
    border: 1px solid rgb(170, 170, 170);
    color:rgb(88, 88, 88);
    font-size:14px;
    border-radius:5px;    
}
QPushButton#Button:hover{
    background-color:rgb(255, 255, 255);
    border: 1px solid rgb(42, 42, 42);
    color:rgb(42, 42, 42);
}

QPushButton#Button:pressed{
    background-color: rgb(255, 146, 62);
    border: 1px solid rgb(255, 146, 62);
    color:rgb(255, 255, 255);
}


QPushButton#DeleteTextButton{
    background:transparent;
    color :rgb(254, 107, 107);
    font-size:14px;
}

QPushButton#DeleteTextButton:hover{
    color :rgb(77, 77, 77);
}

QPushButton#DeleteTextButton:pressed{
    color :rgb(255, 146, 62);
}

QPushButton#DeleteTextButton:disabled{
    color :rgb(225, 225, 225);
}

QPushButton#TextButton{
    background:transparent;
    color :rgb(108, 162, 234);
    font-size:14px;
}

QPushButton#TextButton:hover{
    color :rgb(77, 77, 77);
}

QPushButton#TextButton:pressed{
    color :rgb(255, 146, 62);
}

QPushButton#TextButton:disabled{
    color :rgb(225, 225, 225);
}


QPushButton#SerachButton{
    border-image:url(:/other/images/serach_normal.png);
}

QPushButton#SerachButton:hover{
    border-image:url(:/other/images/serach_hover.png);
}

QPushButton#SerachButton:pressed{
